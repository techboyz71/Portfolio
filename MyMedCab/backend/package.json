{
    "name": "mymedcab2.0-backend",
    "version": "1.0.0",
    "description": "Backend for MyMedCab - Express, TS, PostgreSQL (via Amazon RDS)",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js"
    },
    "keywords": [
        "express",
        "typescript",
        "postgresql",
        "rds",
        "mymedcab"
    ],
    "author": "Josue Quinonez",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.2",
        "pg": "^8.11.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.23",
        "@types/node": "^20.12.7",
        "@types/pg": "^8.10.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.2"
    }
